  \item [{$\mathcal{R} \mid \mathbf{R}$}]\begingroup Abstract operator and matrix representation of a rotation\nomeqref {0}
		\nompageref{13}
  \item [{$\mathcal{F} \mid \mathbf{F}$}]\begingroup Abstract operator and matrix representation of a reflection\nomeqref {0}
		\nompageref{13}
  \item [{$\mathcal{Q} \mid \mathbf{Q}$}]\begingroup Abstract operator and matrix representation of an element of the orthogonal group (either a rotation or a reflection)\nomeqref {0}
		\nompageref{13}
